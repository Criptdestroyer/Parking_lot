#!/usr/bin/env bash
args=${1:-"iteractive"}

if [ $args = "iteractive" ]; then
    java -cp target/Parking_lot-1.0-SNAPSHOT.jar com.gojek.App $args
    
else
    if [ -s $args ];then
        java -cp target/Parking_lot-1.0-SNAPSHOT.jar com.gojek.App $args
        
    else
        echo "invalid filename"
        exit
    fi
fi